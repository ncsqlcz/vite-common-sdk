{
  "name": "vite-common-sdk",
  "private": true,
  "version": "0.1.0",
  "main": "./dist/vite-vanilla-ts-lib-starter.cjs",
  "module": "./dist/vite-vanilla-ts-lib-starter.mjs",
  "scripts": {
    "dev": "vite",
    "dev:open": "vite --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint src --fix --ext .js,.ts,.jsx,.tsx,.vue",
    "lint:fix": "eslint . --fix",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@vueuse/components": "^10.0.0",
    "@vueuse/core": "^10.0.0",
    "axios": "^1.5.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.43.0",
    "@arco-design/web-vue": "^2.52.0",
    "@arco-plugins/vite-vue": "^1.4.5",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/echarts": "^4.9.18",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-vue": "^4.3.4",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-define-config": "^1.23.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^7.0.4",
    "less": "^4.2.0",
    "lint-staged": "^12.5.0",
    "mockjs": "^1.1.0",
    "path": "^0.12.7",
    "plop": "^3.1.2",
    "postcss": "^8.4.30",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "stylelint": "^15.10.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-less": "^2.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-mock": "^2.9.8",
    "vite-plugin-require-transform": "^1.0.21",
    "vue-tsc": "^1.8.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">= 16.9.0"
  }
}
